{
    "author": "Steven Olmsted <steven.olmsted@ibigroup.com>",
    "babel": {
        "plugins": [
            "transform-runtime"
        ],
        "presets": [
            "es2015"
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/ibi-group/isotropic-pubsub/issues",
    "contributors": [
        "Duane Sibilly <duane.sibilly@ibigroup.com>"
    ],
    "dependencies": {
        "babel-runtime": "~6.9.1",
        "core-js": "~2.4.0",
        "isotropic-make": "~0.2.0",
        "isotropic-property-chainer": "~0.2.0"
    },
    "description": "Pubsub event system implementation.",
    "devDependencies": {
        "babel-cli": "~6.10.1",
        "babel-istanbul": "~0.11.0",
        "babel-plugin-transform-runtime": "~6.9.0",
        "babel-preset-es2015": "~6.9.0",
        "chai": "~3.5.0",
        "eslint": "~3.0.1",
        "eslint-config-isotropic": "~0.2.0",
        "in-publish": "~2.0.0",
        "mocha": "~2.5.3"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true,
            "node": true
        },
        "extends": "isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "globalReturn": false,
                "jsx": false
            },
            "ecmaVersion": 6,
            "sourceType": "module"
        },
        "root": true
    },
    "homepage": "https://github.com/ibi-group/isotropic-pubsub",
    "license": "BSD-3-Clause",
    "main": "lib/pubsub.js",
    "name": "isotropic-pubsub",
    "repository": "ibi-group/isotropic-pubsub",
    "scripts": {
        "build": "babel js -d lib",
        "lint": "eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || babel-istanbul check-coverage --branches 85 --functions 95 --lines 95 --statements 95",
        "prepublish": "not-in-install && npm test --coverage && npm run build",
        "pretest": "npm run lint",
        "test": "babel-node ./node_modules/.bin/babel-istanbul test _mocha"
    },
    "version": "0.2.0"
}
