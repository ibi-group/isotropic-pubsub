{
    "author": "Steven Olmsted <steven.olmsted@ibigroup.com>",
    "babel": {
        "env": {
            "build": {
                "presets": [
                    [
                        "babel-preset-minify",
                        {
                            "mangle": false
                        }
                    ]
                ]
            },
            "test": {
                "plugins": [
                    "babel-plugin-istanbul"
                ]
            }
        },
        "plugins": [
            "babel-plugin-transform-line"
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/ibi-group/isotropic-pubsub/issues",
    "contributors": [
        "Duane Sibilly <duane.sibilly@ibigroup.com>"
    ],
    "dependencies": {
        "isotropic-make": "~0.11.0",
        "isotropic-property-chainer": "~0.11.0"
    },
    "description": "Pubsub event system implementation",
    "devDependencies": {
        "@babel/cli": "~7.10.5",
        "@babel/core": "~7.10.5",
        "babel-plugin-istanbul": "~6.0.0",
        "babel-plugin-transform-line": "~0.9.0",
        "@babel/preset-env": "~7.10.4",
        "babel-preset-minify": "~0.5.1",
        "@babel/register": "~7.10.5",
        "chai": "~4.2.0",
        "cross-env": "~7.0.2",
        "eslint": "~7.5.0",
        "eslint-plugin-isotropic": "~0.6.0",
        "mocha": "~8.0.1",
        "nyc": "~15.1.0"
    },
    "engines": {
        "node": "^12.18.3",
        "npm": "^6.14.6"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "extends": "plugin:isotropic/isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2020,
            "sourceType": "module"
        },
        "plugins": [
            "isotropic"
        ],
        "root": true
    },
    "files": [
        "lib"
    ],
    "homepage": "https://github.com/ibi-group/isotropic-pubsub",
    "keywords": [
        "isotropic"
    ],
    "license": "BSD-3-Clause",
    "main": "lib/pubsub.js",
    "name": "isotropic-pubsub",
    "nyc": {
        "branches": 90,
        "check-coverage": false,
        "functions": 100,
        "instrument": false,
        "lines": 99,
        "per-file": false,
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "require": [
            "@babel/register"
        ],
        "sourceMap": false,
        "statements": 99
    },
    "repository": "ibi-group/isotropic-pubsub",
    "scripts": {
        "build": "cross-env BABEL_ENV=build babel js -d lib",
        "lint": "eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || nyc check-coverage",
        "prepare": "npm run build",
        "prepublishOnly": "npm test --coverage",
        "pretest": "npm run lint",
        "test": "cross-env BABEL_ENV=test nyc mocha"
    },
    "version": "0.12.0"
}
